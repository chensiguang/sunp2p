@charset "utf-8";
/* CSS Document */
* {
	margin: 0;
	padding: 0;
	font-weight: normal;
	font-style: normal;
}

button,input,select,textarea{  
    font-size:100%;    
    margin: 0;  
    padding:0;
    outline: 0 none;  
    vertical-align: baseline;  
    *vertical-align: middle;  
} 

ol,ul {
	list-style: none;
}

body {
	font-family: 'Microsoft YaHei', Verdana, Geneva, sans-serif;
	font-size:12px;
}

.clearfix:after {
	display: block;
	content: '';
	height: 0;
	clear: both;
	visibility: hidden;
}

.clearfix {
	zoom: 1;
}

.over-hid {
	overflow:hidden;
}

.content {
	width:100%;
	min-width: 1000px;	
}

.float-l {
	float:left;
}

.float-r {
	float:right;
}

/************ header ***************/
.header {
	width: 100%;
	min-width: 1024px;
	_width:expression((documentElement.clientWidth < 1024) ? "1024px" : "100%" );
	background:url(../../images/top-bg.png) repeat-x;
	padding-top:3px;
	color: #fff;
	font-size: 14px;
}

.top-content {
	height:67px;
	width: 100%;
	min-width: 1024px;
	_width:expression((documentElement.clientWidth < 1024) ? "1024px" : "100%" );
	background:#399bf9;
}

.logo,
.nav-10000,.nav-20000,.nav-30000,.nav-40000,.nav-50000,
.nav-10000-on,.nav-20000-on,.nav-30000-on,.nav-40000-on,.nav-50000-on,
.user-action,.login-status,.login-arrow-down,.login-arrow-up {
	background-image:url(../../images/top-bg.png);
	background-repeat:no-repeat;
}

.logo {
	float:left;
	display:inline-block;
	margin:20px 0 0 29px;
	width:270px;
	height:30px;
	background-position:0 -54px;
}

.nav {
	float: left;
	padding:14px 0 0 54px;
}

.nav li {
	float: left;
	display: inline;
	width:90px;
	height:38px;
	line-height:45px;
	position:relative;
	padding-left:40px;
	cursor:pointer;
}

.nav li a {
	color: #fff;
	text-decoration: none;
}

.nav-icon {
	position:absolute;
	left:0;
	top:0;
	width:38px;
	height:38px;
	display:block;
}

.nav-10000 {                         /*nav-***里面的“***”对应${rst.privilege_id}*/
	background-position:-76px -3px;
}

.nav-10000-on {
	background-position:-114px -3px;
}

.nav-20000 {
	background-position:-228px -3px;
}

.nav-20000-on {
	background-position:-266px -3px;
}

.nav-30000 {
	background-position:-152px -3px;
}

.nav-30000-on {
	background-position:-190px -3px;
}

.nav-40000 {
	background-position:0 -3px;
}

.nav-40000-on {
	background-position:-38px -3px;
}

.nav-50000 {
	background-position:-304px -3px;
}

.nav-50000-on {
	background-position:-342px -3px;
}

.user-action {
	float:right;
	padding:25px 0 0 20px;
	width:55px;
	height:42px;
	background-position:-494px -12px;
}

.login-status {
	width:39px;
	height:26px;
	background-position:-353px -54px;
	position:relative;
	cursor:pointer;
}

.login-out {
	position:absolute;
	top:34px;
	right:0;
	padding:5px;
	width:80px;
	text-align:center;
	background-color:#399bf9;
	display:none;
}

.login-out a {
	color:#fff;
	text-decoration:none;
}

.login-arrow-up,.login-arrow-down {
	display:block;
	width:7px;
	height:7px;
	font-size:0;
	position:absolute;
	top:18px;
	left:32px;
}

.login-arrow-down {
	background-position:-432px -72px;
	
}

.login-arrow-up {
	background-position:-474px -72px;
}

/************** main-wrap *****************/
.main-title {
	background-color:#e9ebef;
	border-bottom:solid 1px #c5c5c5;
	padding:5px 10px;	
	font-size:12px;
	font-weight:bold;
	color:#4d4d4d;
}

.main-actions {
	padding:10px 0 0 20px;
	border-bottom:1px solid #ccc;
}

.non-bottom {
	border-bottom:0;
}

.main-actions li {
	position:relative;
	float:left;
	height:30px;
	padding:0 25px 10px 0;
	line-height:30px;
	width:auto;
	white-space:nowrap;
}

.main-actions li label {
	display:inline-block;
	width:90px;
	text-align:right;
}

.main-actions li label.extra{
	width:95px;
}

.main-actions li input[type="checkbox"] {
	vertical-align:text-bottom;
	margin-bottom:2px;
}

.text {
	height:28px;
	line-height:28px;
	width:200px;
	border-style:solid;
	border-width:1px;
	border-color:#d4d4d4;
	border-radius:4px;
	margin:0 auto;
	vertical-align:middle;
}

.tool-text {
	width:220px;
	height:23px;
	line-height:23px;
	border-style:solid;
	border-width:1px;
	border-color:#d4d4d4;
	border-radius:4px;
	margin:0 auto;
	vertical-align:middle;
}

.select {
	width:202px;
	height:30px;
	line-height:30px;
	border-width:1px;
	border-color:#d4d4d4;
	border-radius:4px;
	margin:0 auto;
	vertical-align:middle;
}

.tool-select {
	width:222px;
	height:25px;
	line-height:25px;
	border-width:1px;
	border-color:#d4d4d4;
	border-radius:4px;
	margin:0 auto;
	vertical-align:middle;
}

.sch-button	{
	display:block;
	height:28px;
	line-height:28px;
	padding-left:35px;
	width:48px;
	background:url(../../images/search-icon.png) no-repeat 16px 6px #fff;
	border:solid 1px #00bbff;
	border-radius:4px;
	-webkit-border-radius:4px;
	color:#1cc2ff;
	cursor:pointer;
}

.toolBar {
	height:28px;
	line-height:28px;
	padding-left:30px;
	position:relative;
	background:url(../../images/toolbar-bg.png) bottom repeat-x;
}

.toolbar-icon {
	position:absolute;
	left:10px;
	top:7px;
	display:block;
	width:14px;
	height:14px;
	background:url(../../images/toolbar-icon.png) no-repeat;
}

.privileges {
	position:absolute;
	right:0;
	top:0;
}

table {
		display:table;
}

.data-content {
	border-collapse:collapse;
	font-size:12px;
	width:100%;
	border:solid 1px #dfe1e3;
	margin-left:-1px;
}

.data-content th,.data-content td {
	border:solid 1px #dfe1e3;
}

.data-content a {
	text-decoration:none;
	color:#000;
}

.data-content a:hover {
	text-decoration:underline;
}

.data-content th {
	padding:5px;
	background:#f7f7f7;
	color:#333;
}
.data-content td {
	padding:5px;
	background:#fafbfb;
	color:#666;
}

.data-content tr:hover {
	background:#e1f0fe;
}
.data-content tr:hover td {
	background:none;
}
.checkedTr {
	background:#e1f0fe;
}

.checkedTr td {
	background:none;
}
.button {
	padding:2px 8px;
	display:inline-block;
	border:1px solid #ccc;
	color:#000;
	text-decoration:none;
	cursor:pointer;
	line-height:24px;
	border-top: 0;
	border-bottom: 0;
}
.toolBar a:hover {
	background:#666;
	color:#fff;
}
	
.button_normal {
	padding:4px 20px
}

.checkBox {
	width:30px;
}

.page-tag {
	height:28px;
	line-height:28px;
	padding-left:20px;
	position:relative;
	background:url(../../images/toolbar-bg.png) bottom repeat-x;
	border-bottom:solid 1px #ccc;
}

.page-num {
	float:right;
	padding-right:20px;
}

.window-pop {
	width:400px;
	margin:0 auto;
	font-size:12px;
	padding-top:15px;
}

.window-box {
	height:40px; 
	line-height:40px; 
	padding-left:20px; 
	border-bottom:1px solid #ccc;
}

.pop-list li {
	height:35px;
	line-height:35px;
	position:relative;
	text-align:left;
}

.pop-list li label {
	display:inline-block;
	width:110px;
	text-align:right;
}

.tool-buttons {
	text-align:center;
	padding-top:15px;
	height:50px;
}

.tool-btn {
	font-size:12px;
	font-family:'Microsoft Yahei';
	display: inline-block;
	vertical-align:top;
	line-height:24px;
	*height:26px;
	*line-height:20px;
	padding:0 5px;
	border: 1px solid #bbb;
	-moz-border-radius: 5px 5px 5px 5px;
	-webkit-border-radius: 5px 5px 5px 5px;
	border-radius: 5px 5px 5px 5px;
	cursor:pointer;
}

.login-bg {
	/* position:relative;
	top:0;
	left:0; */
	position:absolute; 
	width:100%; 
	height:100%; 
	z-index:-1;
}

.login-bg-img {
	width:100%;
	height:100%;
	min-width:1024px;
}

.login-main {
	width:470px;
	position: absolute; 
	top: 50%; 
	left: 50%; 
	margin: -280px 0 0 -235px;
	border:3px solid #d5edff;
	border-radius:10px;
}

.login-content {
	padding:80px 40px;
}

.login-face,.login-title {
	text-align:center; 
	margin-bottom:20px;
}

.user-login-info {
	width:347px;
	height:115px;
	border-radius:10px;
	background-color:#f1f1f1;
	padding:10px 20px;
	margin-bottom:20px;
	color:#828282;
}

.user-name {
	padding-bottom: 10px;
	border-bottom: 1px dotted #ccc;
}

.user-pwd {
	padding-top: 10px;
}

.user-name-input,.user-pwd-input {
	width: 260px; 
	height: 45px; 
	line-height:45px; 
	border:0; 
	background:none; 
	margin:0 auto; 
	vertical-align:middle;
}

.user-login-button {
	padding:20px 40px;
}

.user-login,.user-reg {
	display:inline-block;
	width:300px;
	height:42px;
	line-height:42px;
	text-align:center;
	border-radius:5px;
	color:#fff;
	cursor:pointer;
	font-size:20px;
}

.user-login {
	background:#fb9c87;
	border:0;
	font-family:'Microsoft YaHei';
}

.user-reg {
	background:#eadedb;
	color:#939393;
}

.result-page {
	background:url(../../images/result-bg.png) center bottom no-repeat #f4f3ef;
	height:215px;
	padding-top:120px;
	text-align:center;
}

.result-content {
	margin:0 auto;
	width:470px;
	text-align:left;
}

.result-face {
	float:left;
	width:110px;
	height:110px;
	display:block;
}

.result-msg {
	margin-left:130px;
	vertical-align:top;
}

.msg-strong {
	font-size:36px;
	color:#399bf9;
	line-height:70px;
}

.msg-more {
	font-size:18px;
	color:#399bf9;
}

.result-button {
	display:block;
	border:1px solid #399bf9;
	border-radius:10px;
	line-height:30px;
	height:30px;
	width:60px;
	text-align:center;
	background:#fff;
	color:#399bf9;
	text-decoration:none;
	margin-top:20px;
}